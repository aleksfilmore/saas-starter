{
  "expo": {
    "name": "CTRL+ALT+BLOCK",
    "slug": "ctrlaltblock",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "dark",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#081F20"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.ctrlaltblock.app",
      "infoPlist": {
        "UIBackgroundModes": ["background-fetch", "background-processing"],
        "BGTaskSchedulerPermittedIdentifiers": ["com.ctrlaltblock.app.refresh"]
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#081F20"
      },
      "package": "com.ctrlaltblock.app",
      "permissions": [
        "NOTIFICATIONS",
        "VIBRATE",
        "WAKE_LOCK"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-notifications",
      "expo-secure-store"
    ],
    "extra": {
      "eas": {
        "projectId": "ctrlaltblock-mobile"
      }
    }
  }
}
